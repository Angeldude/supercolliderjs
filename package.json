{
  "name": "supercolliderjs",
  "description": "Tools for working with the SuperCollider music language environment",
  "version": "0.7.0",
  "author": "Felix <crucialfelix@gmail.com>",
  "contributors": [
    {
      "name": "Chris Sattinger",
      "email": "crucialfelix@gmail.com"
    }
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "js-yaml": "^3.3.1",
    "node-uuid": "^1.4.3",
    "osc-min": "^0.2.0",
    "q": "^1.2.0",
    "temp": "~0.8.1",
    "underscore": "^1.8.3",
    "untildify": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "jshint": "^2.8.0",
    "babel-jest": "*",
    "jest-cli": "*"
  },
  "license": "MIT",
  "keywords": [
    "supercollider",
    "synthesis",
    "music",
    "sound"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/crucialfelix/supercolliderjs"
  },
  "main": "index",
  "bin": {
    "supercollider-server": "./bin/scsynth.js",
    "supercollider": "./bin/sclang.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "jshint src/* bin/* examples/*",
    "compile": "babel -d lib/js src/",
    "prepublish": "npm run compile"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"]
  },
  "engines": {
    "node": ">=0.10.0"
  }
