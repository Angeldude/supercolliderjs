{
  "name": "supercolliderjs",
  "description": "Tools for working with the SuperCollider music language environment",
  "version": "0.8.2",
  "author": "Felix <crucialfelix@gmail.com>",
  "contributors": [
    {
      "name": "Chris Sattinger",
      "email": "crucialfelix@gmail.com"
    }
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "immutable": "^3.7.5",
    "js-yaml": "^3.3.1",
    "ncp": "^2.0.0",
    "node-uuid": "^1.4.3",
    "osc-min": "^0.2.0",
    "q": "^1.4.1",
    "temp": "~0.8.3",
    "underscore": "^1.8.3",
    "untildify": "^2.1.0"
  },
  "devDependencies": {
    "babel": "^5.5.29",
    "babel-jest": "5.3.0",
    "jest-cli": "0.6.1",
    "jscs": "^2.5.0",
    "jshint": "^2.8.0"
  },
  "license": "MIT",
  "keywords": [
    "supercollider",
    "synthesis",
    "music",
    "sound"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/crucialfelix/supercolliderjs"
  },
  "main": "index",
  "bin": {
    "supercollider-server": "./bin/scsynth.js",
    "supercollider": "./bin/sclang.js",
    "export-supercollider": "./bin/export-supercollider.js",
    "compile-synthdefs": "./bin/compile-synthdefs.js"
  },
  "scripts": {
    "test": "jest && npm run lint && npm run jsstyle",
    "lint": "jshint src/*.es6 bin/*.js examples/*.js",
    "jsstyle": "jscs src/*.es6 bin/*.js examples/*.js",
    "compile": "cd src && babel *.es6 --source-maps --out-dir ../lib/js",
    "copy": "cd src && cp *.json ../lib/js/",
    "watch": "cd src && babel *.es6 --watch --source-maps --out-dir ../lib/js",
    "prepublish": "npm run compile && npm run copy"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "testPathDirs": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "collectCoverage": true
  },
  "engines": {
    "node": ">=0.11.0 <5"
  }
}
